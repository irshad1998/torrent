<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppThemeLight" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/homePrimaryDark</item>
        <item name="colorPrimaryDark">@color/homePrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/cf</item>
        <item name="android:fontFamily">@font/f</item>
    </style>

    <!-- Base application theme. -->
    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/splashPrimary</item>
        <item name="colorPrimaryDark">@color/splashPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

